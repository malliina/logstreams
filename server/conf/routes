GET         /                          controllers.Logs.index
GET         /ping                      controllers.Logs.ping
GET         /sources                   controllers.Logs.sources
GET         /users                     controllers.Logs.allSources
POST        /users                     controllers.Logs.addUser
POST        /users/:user/delete        controllers.Logs.removeUser(user: com.malliina.play.models.Username)
GET         /ws/logs                   controllers.SocketsBundle.listenerSocket
GET         /ws/admins                 controllers.SocketsBundle.adminSocket
GET         /ws/sources                controllers.SocketsBundle.sourceSocket
GET         /oauth                     controllers.OAuth.initiate
GET         /oauthcb                   controllers.OAuth.redirResponse
GET         /assets/*file              controllers.Logs.versioned(path = "/public", file)
